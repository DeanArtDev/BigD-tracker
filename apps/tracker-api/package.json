{
  "name": "@big-d/tracker-api",
  "version": "0.0.1",
  "description": "",
  "author": "deankorobchenko@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "=== DEV ===": "",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "db:up:dev": "docker-compose --env-file .env.development up -d",
    "db:down:dev": "docker-compose --env-file .env.development down -v",
    "=== RUN ===": "",
    "build": "nest build",
    "start": "nest start",
    "start:prod": "node dist/main",
    "=== TOOLS ===": "",
    "migrate": "ts-node tools/migrator.ts",
    "typegen": "kysely-codegen --out-file ./src/shared/modules/db/types.d.ts --env-file .env.development --dialect postgres",
    "=== CI ===": "",
    "format": "prettier \"src/**/*.ts\" \"test/**/*.ts\" \"db/**/*.ts\" \"tools/**/*.ts\" --check",
    "format:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"db/**/*.ts\" \"tools/**/*.ts\" --check",
    "lint": "pnpm format && eslint \"{src,apps,libs,test,tools,db}/**/*.ts\"",
    "lint:fix": "pnpm format:fix && eslint \"{src,apps,libs,test,tools,db}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@big-d/api-exception": "workspace:*",
    "@nestjs/common": "catalog:",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@types/pg": "^8.15.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "kysely": "^0.28.2",
    "kysely-ctl": "^0.12.2",
    "lodash-es": "catalog:",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "catalog:",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "catalog:",
    "@types/node": "catalog:",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.5.0",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "kysely-codegen": "^0.18.5",
    "prettier": "catalog:",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "catalog:",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": "22.15.0",
    "pnpm": "10.10.0"
  }
}
